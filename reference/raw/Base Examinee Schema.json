{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "examineeCode": {
      "type": "string"
    },
    "passcode": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dob": {
      "type": "object",
      "required": [
        "year",
        "month",
        "day"
      ],
      "properties": {
        "year": {
          "type": "integer"
        },
        "month": {
          "type": "integer"
        },
        "day": {
          "type": "integer"
        }
      }
    },
    "gender": {
      "type": "integer"
    },
    "timeAdded": {
      "type": "string"
    },
    "imagePath": {
      "type": [
        "string",
        "null"
      ]
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "mappedExams": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "addedGroups": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "proctorApprovedExams": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "proctorExemptedExams": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "fullName": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "examineeCode",
    "passcode",
    "firstName",
    "middleName",
    "lastName",
    "dob",
    "gender",
    "timeAdded",
    "imagePath",
    "phoneNumber",
    "email",
    "mappedExams",
    "addedGroups",
    "proctorApprovedExams",
    "proctorExemptedExams",
    "fullName"
  ]
}